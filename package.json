		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"remark-external-links": "^9.0.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.12",
		"tslib": "^2.3.1",
